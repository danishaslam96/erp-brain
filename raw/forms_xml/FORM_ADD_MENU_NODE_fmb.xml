      <ProgramUnit Name="FUN_GEN_WIZMENU_DET_SCRIPT" ProgramUnitText="FUNCTION FUN_GEN_WIZMENU_DET_SCRIPT (PARM_VNODE_ID NUMBER) RETURN VARCHAR IS&amp;#10;&#x9;LVCH_SQL VARCHAR2(10000);&amp;#10;&#x9;LVCH_NODE_ID VARCHAR2(100);&amp;#10;&#x9;LVCH_NODE_NAME VARCHAR2(100);&amp;#10;&#x9;LVCH_WIZMENU_ID VARCHAR2(100);&amp;#10;&#x9;LVCH_NODE_PARENT VARCHAR2(100);&amp;#10;&#x9;LVCH_NODE_DESC VARCHAR2(100);&amp;#10;&#x9;LVCH_NODE_PATH VARCHAR2(200);&amp;#10;&#x9;LVCH_MENU_PARAMETERS VARCHAR2(1000);&amp;#10;&#x9;LVCH_SHOW_IN_MENU VARCHAR2(2);&amp;#10;&#x9;LVCH_MAIN_FUNCTION_ID VARCHAR2(100);&amp;#10;&#x9;LF varchar2(1):='&amp;#10;';&amp;#10;BEGIN&amp;#10;&#x9;SELECT &#x9;TO_CHAR(T.WIZMENU_ID), TO_CHAR(T.NODE_ID), T.NODE_NAME, T.NODE_DESC, T.NODE_PATH,&amp;#10;       &#x9;&#x9;TO_CHAR(T.NODE_PARENT), T.SHOW_IN_MENU, T.MENU_PARAMETERS, T.MAIN_FUNCTION_ID&amp;#10;&#x9;INTO   &#x9;LVCH_WIZMENU_ID,LVCH_NODE_ID,LVCH_NODE_NAME,LVCH_NODE_DESC,LVCH_NODE_PATH,&amp;#10;&#x9;&#x9;&#x9;&#x9;&#x9;LVCH_NODE_PARENT,LVCH_SHOW_IN_MENU,LVCH_MENU_PARAMETERS,LVCH_MAIN_FUNCTION_ID&amp;#10;&#x9;FROM WIZMENU_DET T&amp;#10;&#x9;WHERE T.NODE_ID = PARM_VNODE_ID;&amp;#10;&#x9;&amp;#10;  LVCH_SQL := 'prompt importing file...&amp;#10;set feedback off&amp;#10;set define off&amp;#10;prompt Disabling triggers for WIZMENU_DET...&amp;#10;alter table WIZMENU_DET disable all triggers;&amp;#10;prompt Disabling foreign key constraints for WIZMENU_DET...&amp;#10;alter table WIZMENU_DET disable constraint FK_WIZMENU_DET_02;&amp;#10;prompt Loading WIZMENU_DET...&amp;#10;prompt ______________________________________________&amp;#10;insert into WIZMENU_DET (WIZMENU_ID, NODE_ID, NODE_NAME, NODE_DESC, NODE_PATH, NODE_PARENT, SHOW_IN_MENU, MENU_PARAMETERS, MAIN_FUNCTION_ID)&amp;#10;values (' ||LVCH_WIZMENU_ID ||','|| LVCH_NODE_ID ||',''' ||LVCH_NODE_NAME || ''',''' ||LVCH_NODE_DESC || ''',''' || LVCH_NODE_PATH ||''',' || NVL(LVCH_NODE_PARENT,'NULL') ||',''' || LVCH_SHOW_IN_MENU || ''',''' || LVCH_MENU_PARAMETERS ||''',' ||NVL(LVCH_MAIN_FUNCTION_ID,'NULL')||');&amp;#10;prompt ______________________________________________&amp;#10;commit;&amp;#10;prompt Enabling foreign key constraints for WIZMENU_DET...&amp;#10;alter table WIZMENU_DET enable constraint FK_WIZMENU_DET_02;&amp;#10;prompt Enabling triggers for WIZMENU_DET...&amp;#10;alter table WIZMENU_DET enable all triggers;&amp;#10;set feedback on&amp;#10;set define on&amp;#10;prompt Done.';&amp;#10;&#x9;&amp;#10;&#x9;RETURN LVCH_SQL;&amp;#10;EXCEPTION&amp;#10;&#x9;WHEN NO_DATA_FOUND THEN&amp;#10;&#x9;&#x9;RETURN NULL;&amp;#10;END;" ProgramUnitType="Function"/>
      <ProgramUnit Name="PRO_MENU" ProgramUnitText="&amp;#10;PROCEDURE PRO_MENU IS&amp;#10;BEGIN&amp;#10;  SELECT MAX(T2.NODE_ID) INTO :TEXT_PREV FROM WIZMENU_DET T2 WHERE T2.NODE_ID &lt; :NODE_ID;&amp;#10;  SELECT T.NODE_NAME INTO :TEXT_PARENT_TITLE  FROM WIZMENU_DET T WHERE T.NODE_ID = :NODE_PARENT;&amp;#10;  SELECT MIN(T2.NODE_ID) INTO :TEXT_NEXT  FROM WIZMENU_DET T2 WHERE T2.NODE_ID > :NODE_ID;&amp;#10;  SELECT T2T.NODE_NAME INTO :TEXT_PREV_DESC FROM WIZMENU_DET T2T WHERE T2T.NODE_ID = (SELECT MAX(T2.NODE_ID) FROM WIZMENU_DET T2 WHERE T2.NODE_ID &lt; :NODE_ID);&amp;#10;  SELECT T2T.NODE_NAME INTO :TEXT_NEXT_DESC FROM WIZMENU_DET T2T WHERE T2T.NODE_ID = (SELECT MIN(T2.NODE_ID) FROM WIZMENU_DET T2 WHERE T2.NODE_ID > :NODE_ID);&amp;#10;END;" ProgramUnitType="Procedure"/>
