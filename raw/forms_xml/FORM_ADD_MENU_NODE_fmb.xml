      <Trigger Name="KEY-CREREC" DirtyInfo="true" TriggerText="declare&amp;#10;&#x9;&#x9;LNUM_PRE NUMBER;&amp;#10;&#x9;&#x9;LNUM_NEXT NUMBER;&amp;#10;&#x9;&#x9;LNUM_N_ID_P NUMBER;&amp;#10;&#x9;&#x9;LNUM_N_ID_N NUMBER;&amp;#10;&#x9;&#x9;LNUM_PARENT NUMBER;&amp;#10;&#x9;&#x9;LNUM_TEMP NUMBER;&amp;#10;&#x9;&#x9;LCHR_NODE_PATH VARCHAR2(100);&amp;#10;&#x9;&#x9;LNUM_COUNT NUMBER;&amp;#10;begin&amp;#10;&#x9;&#x9;LNUM_PRE := :NODE_ID;&amp;#10;&#x9;&#x9;LNUM_PARENT := :NODE_PARENT;&amp;#10;&#x9;&#x9;LCHR_NODE_PATH := :NODE_PATH;&amp;#10;&amp;#10;&#x9;&#x9;IF :PARAMETER.PARM_DISABLED_MODE IS NOT NULL THEN&amp;#10;&#x9;&#x9;&#x9;&#x9;RAISE Form_Trigger_Failure;&amp;#10;&#x9;&#x9;END IF;&amp;#10;&#x9;&#x9;PCKG_MT_MENU_TRGRS.PRO_KEY_CREREC;&amp;#10;&#x9;&#x9;&amp;#10;&#x9;&#x9;:WIZMENU_ID := 2;&amp;#10;&#x9;&#x9;&amp;#10;&#x9;&#x9;IF:PARAMETER.PARM_MODE = PCKG_GCNSTS.GCST_NORMAL_MODE  THEN&amp;#10;&#x9;&#x9;&#x9;:text_save:=1;&amp;#10;&#x9;&#x9;ELSE &amp;#10;&#x9;&#x9;&#x9;:text_save:=0;&amp;#10;&#x9;&#x9;END IF;&amp;#10;&#x9;&#x9;&amp;#10;&#x9;&#x9;:TEXT_PREV := LNUM_PRE;&amp;#10;&#x9;&#x9;IF LNUM_PRE IS NOT NULL THEN&amp;#10;&#x9;&#x9;&#x9;SELECT T2T.NODE_NAME INTO :TEXT_PREV_DESC FROM WIZMENU_DET T2T WHERE T2T.NODE_ID = LNUM_PRE;&amp;#10;&#x9;&#x9;END IF;&amp;#10;&#x9;&#x9;&amp;#10;&#x9;&#x9;BEGIN&amp;#10;&#x9;&#x9;&#x9;&#x9;--CHECK WHETHER CURRENT NODE + 0.01 EXISTS&amp;#10;&#x9;&#x9;&#x9;&#x9;SELECT COUNT(1) INTO LNUM_COUNT&amp;#10;&#x9;&#x9;&#x9;&#x9;FROM WIZMENU_DET T&amp;#10;&#x9;&#x9;&#x9;&#x9;WHERE T.NODE_ID =LNUM_PRE+0.01;&amp;#10;&#x9;&#x9;&#x9;&#x9;--GET NEXT NODE ID&amp;#10;&#x9;&#x9;&#x9;&#x9;SELECT MIN(T2.NODE_ID) INTO LNUM_NEXT FROM WIZMENU_DET T2 WHERE T2.NODE_ID > LNUM_PRE;&amp;#10;&#x9;&#x9;&#x9;&#x9;:TEXT_NEXT := LNUM_NEXT;&amp;#10;&#x9;&#x9;&#x9;&#x9;SELECT T2T.NODE_NAME INTO :TEXT_NEXT_DESC FROM WIZMENU_DET T2T WHERE T2T.NODE_ID = LNUM_NEXT;&amp;#10;&#x9;&#x9;EXCEPTION WHEN NO_DATA_FOUND THEN&amp;#10;&#x9;&#x9;&#x9; :TEXT_NEXT := NULL;&amp;#10;&#x9;&#x9;&#x9; :TEXT_NEXT_DESC := NULL;&amp;#10;&#x9;&#x9;END;&amp;#10;&#x9;&#x9;&amp;#10;&#x9;&#x9;IF LNUM_COUNT &lt; 1 THEN&amp;#10;&#x9;&#x9;&#x9;&#x9;:NODE_ID := LNUM_PRE+0.01;&amp;#10;&#x9;&#x9;--IF GENERATED ID ALREADY EXIST GET MAX NODE ID OF THAT PARENT+1&amp;#10;&#x9;&#x9;ELSE&amp;#10;&#x9;&#x9;&#x9;&#x9;SELECT MAX(T.NODE_ID+0.01) INTO :NODE_ID&amp;#10;&#x9;&#x9;&#x9;&#x9;FROM WIZMENU_DET T&amp;#10;&#x9;&#x9;&#x9;&#x9;WHERE T.NODE_PARENT = NVL(LNUM_PARENT,LNUM_PRE);&amp;#10;&#x9;&#x9;END IF;&amp;#10;&#x9;&#x9;&amp;#10;&#x9;&#x9;/*&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&amp;#10;&#x9;&#x9;LNUM_N_ID_P := get_value_at(LNUM_PRE,2,'.');&amp;#10;&#x9;&#x9;LNUM_N_ID_P := RPAD(LNUM_N_ID_P,2,'0');&amp;#10;&#x9;&#x9;LNUM_N_ID_N := get_value_at(LNUM_NEXT,2,'.');&amp;#10;&#x9;&#x9;LNUM_N_ID_N := RPAD(LNUM_N_ID_N,2,'0');&amp;#10;&#x9;&#x9;&amp;#10;&#x9;&#x9;--MESSAGE(LNUM_N_ID_P ||'  ' || LNUM_N_ID_N);&amp;#10;&#x9;&#x9;IF TO_NUMBER(LNUM_N_ID_N) &lt;> (TO_NUMBER(LNUM_N_ID_P)+1) THEN&amp;#10;&#x9;&#x9;&#x9;&#x9;:NODE_ID := get_value_at(LNUM_PRE,1,'.')|| '.' || (TO_NUMBER(LNUM_N_ID_P)+1);&amp;#10;&#x9;&#x9;ELSE&amp;#10;&#x9;&#x9;&#x9;&#x9;BEGIN&amp;#10;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT RPAD(MAX(get_value_at(T2.NODE_ID,2,'.')),2,'0') INTO LNUM_TEMP FROM WIZMENU_DET T2 WHERE T2.NODE_PARENT = LNUM_PARENT;&amp;#10;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;LNUM_TEMP := LNUM_TEMP+1;&amp;#10;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;:NODE_ID := (get_value_at(LNUM_PRE,1,'.')||'.'||LNUM_TEMP);&amp;#10;&#x9;&#x9;&#x9;&#x9;EXCEPTION WHEN NO_DATA_FOUND THEN&amp;#10;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;:NODE_ID := (get_value_at(LNUM_PRE,1,'.')||'.01');&amp;#10;&#x9;&#x9;&#x9;&#x9;END;&amp;#10;&#x9;&#x9;END IF;&amp;#10;&#x9;&#x9;*/&amp;#10;&#x9;&#x9;IF LCHR_NODE_PATH IS NULL THEN&amp;#10;&#x9;&#x9;&#x9;:NODE_PARENT := LNUM_PRE;&amp;#10;&#x9;&#x9;ELSE&amp;#10;&#x9;&#x9;&#x9;:NODE_PARENT := LNUM_PARENT;&amp;#10;&#x9;&#x9;END IF;&amp;#10;end;"/>
